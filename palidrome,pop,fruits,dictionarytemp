#working with sets
set1 = {1,2,3,4}
set2 = {3,4,5,6}
print("union:",set1|set2)
print("intersection:",set1 & set2)
print("difference:",set1-set2)
union: {1, 2, 3, 4, 5, 6}
intersection: {3, 4}
difference: {1, 2}

